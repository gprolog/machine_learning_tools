        <match>SWF_DEBUG_CLASS_INFO_COUNT_LARGER_THAN_MAX</match>
        <match>SWF_DEBUG_METHOD_INFO_COUNT_ZERO</match>
        <match>SWF_DEBUG_CONSTANT_POOL_PARSE_ERROR</match>
